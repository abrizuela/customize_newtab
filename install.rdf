        <em:type>2</em:type> <!-- 2 : Extension -->
        <em:id>customizenewtab@alejandrobrizuela.com.ar</em:id>
        <em:version>1.3.1</em:version>
        <em:name>Customize about:newtab</em:name>
        <em:creator>Alejandro Brizuela</em:creator>
        
